{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "plugins": [
    "import", "jsx-a11y", "react"
  ],
  "rules": {
    "react/forbid-prop-types": "off",
    "react/jsx-filename-extension": "off",
    "react/no-unknown-property": [1, { ignore: ["class", "for"] }]
  }
}
